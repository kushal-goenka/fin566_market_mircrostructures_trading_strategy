<!--
<endpoints> attributes:
    * version - required

    <host> attributes:
        * url - required, address of the host
        * port - optional, defaults to 443 
        * key - api key, required
        * isactive - optional (default true) - skipped if false
        * source - optional (default "twitter", valid values are "twitter" or "stocktwits")
        
        <ontology> attributes:
            * name - required (valid values are ticker,etf,index,forex,future,crypto,stockactivity,etfactivity,forexactivity,futureactivity, or cryptoactivity) 
-->

<endpoints version='1'>

    <host url='api.socialmarketanalytics.com' key='key' isactive='false'>
        <ontology name='ticker' />
        <ontology name='etf' />
        <ontology name='index' />
        <ontology name='forex' />
        <ontology name='future' />
    </host>

    <host url='api3.socialmarketanalytics.com' key='key' isactive='false'>
        <ontology name='stockactivity' />
        <ontology name='etfactivity' />
        <ontology name='forexactivity' />
        <ontology name='futureactivity' />
    </host>

    <host url='api.socialmarketanalytics.com' key='key' isactive='false' source='stocktwits'>
        <ontology name='ticker' />
        <ontology name='etf' />
        <ontology name='index' />
        <ontology name='forex' />
        <ontology name='future' />
        <ontology name='stockactivity' />
        <ontology name='etfactivity' />
        <ontology name='forexactivity' />
        <ontology name='futureactivity' />
    </host>

    <host url='api-cc.socialmarketanalytics.com' key='key' isactive='false'>
        <ontology name='crypto' />
        <ontology name='cryptoactivity' />
    </host>

</endpoints>